{
  "name": "vxe-ui-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:v4": "cd v4 && npm run build",
    "update:v4": "cd v4 && npm install --legacy-peer-deps",
    "update:all": "npm run update:v4",
    "build:all": "npm run build:v4",
    "build:docs": "gulp build_docs && npm run build:zip",
    "build:vxe": "gulp build_docs && gulp build_vxe_docs && npm run build:zip",
    "build:zip": "gulp build_zip"
  },
  "devDependencies": {
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-concat": "2.6.1",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.1.4",
    "gulp-zip": "^5.1.0",
    "uglify-js": "^3.17.4"
  }
}
